div(class="header")
    div(class="header__container")
        div(class="header__logo")
            div(class="header__logo_icon")
                img(src=imageUrl alt="Логотип :)" id="logo")
        div(class="header__navbar")
            each cont in menuContent
                div(class="element-background")
                    a(class="header__navbar_element" id=cont.link)= cont.title